{
  "manifest_version": 3,
  "version": "0.5.2",
  "name": "Live Stream Downloader Unleashed!!",
  "description": "__MSG_description__",
  "default_locale": "en",
  "permissions": [
    "storage",
    "contextMenus",
    "webRequest",
    "declarativeNetRequestWithHostAccess",
    "scripting",
    "*://*/*.flv*",
    "*://*/*.avi*",
    "*://*/*.wmv*",
    "*://*/*.mov*",
    "*://*/*.mp4*",
    "*://*/*.pcm*",
    "*://*/*.wav*",
    "*://*/*.mp3*",
    "*://*/*.aac*",
    "*://*/*.ogg*",
    "*://*/*.wma*",
    "*://*/*.m3u8*",
    "unlimitedStorage"
  ],
  "optional_permissions": [],
  "homepage_url": "https://github.com/helloyanis/live-stream-downloader-unleashed",
  "background": {
    "service_worker": "worker.js",
    "scripts": [
      "network/core.js",
      "network/icon.js",
      "context.js",
      "plugins/blob-detector/core.js",
      "worker.js"
    ]
  },
  "icons": {
      "96": "/data/icons/download_off.svg",
      "128": "/data/icons/download_off.svg"
  },
    
  
  "browser_action": {
    "default_title": "Download HLS Streams"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{2ea2bfef-af69-4427-909c-34e1f3f5a418}",
      "strict_min_version": "128.0"
    }
  }

}